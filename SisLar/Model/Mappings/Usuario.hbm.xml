<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="SisLar.Model.Entities.Usuario,SisLar" table="SIS_USUARIO" lazy="true">
    <id name="Handle" column="HANDLE" type="int">
      <generator class="native" />
    </id>
    <property name="Nome" column="NOME" type="string" not-null="true" />
    <property name="Id" column="ID" type="string" />
    <property name="DataCadastro" column="DATACADASTRO" type="DateTime" />
    <property name="Login" column="LOGIN" type="string" />
    <property name="Senha" column="SENHA" type="string" />
  </class>
</hibernate-mapping>